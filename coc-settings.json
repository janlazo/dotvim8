{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.source.tag.filetypes": ["c", "cpp"],
  "html.filetypes": ["html"],
  "json.schemas": [
    {
      "fileMatch": ["*.geojson"],
      "url": "http://json.schemastore.org/geojson"
    }
  ],
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "phan": {
      "command": "phan",
      "filetypes": ["php"],
      "args": [
        "--allow-polyfill-parser",
        "--language-server-on-stdin",
        "--language-server-allow-missing-pcntl",
        "--language-server-enable-go-to-definition"
      ]
    }
  },
  "less.validate": false,
  "wxss.validate": false,
  "yaml.schemas": {
  }
}
