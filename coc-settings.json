{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.source.tag.filetypes": ["c", "cpp"],
  "html.filetypes": ["html"],
  "json.schemas": [
    {
      "fileMatch": ["*.geojson"],
      "url": "http://json.schemastore.org/geojson"
    }
  ],
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "less.validate": false,
  "wxss.validate": false,
  "yaml.schemas": {
  }
}
